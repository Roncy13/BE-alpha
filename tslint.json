{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "max-classes-per-file": [false],
      "no-console": true,
      "indent": [true, "spaces", 2],
      "one-line": false,
      "whitespace": [false, "check-branch", "check-operator", "check-typecast"]
  },
  "rulesDirectory": []
}